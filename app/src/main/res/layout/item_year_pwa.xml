<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:orientation="vertical" xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/white"
    android:id="@+id/re"
    >
    <View
        android:id="@+id/view"
        android:layout_width="match_parent"
        android:layout_height="@dimen/common8dp"
        android:background="@color/bg_color"/>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/common45dp">
        <TextView
            android:layout_marginLeft="@dimen/common15dp"
            android:layout_centerVertical="true"
            android:layout_width="@dimen/common2dp"
            android:layout_height="@dimen/common15dp"
            android:background="@color/text_ff96"/>
        <TextView
            android:id="@+id/car_letter"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/common31dp"
            android:textColor="@color/text_33"
            android:textSize="@dimen/text_size_17sp"
            android:layout_centerVertical="true"
            android:text="A"
            />
    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/common1dp"
        android:background="@color/bg_color"/>

</LinearLayout>