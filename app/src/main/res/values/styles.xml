<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="ll_normal_horizontal">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/common45dp</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_marginTop">@dimen/common1dp</item>
        <item name="android:background">@color/white</item>
    </style>

    <style name="black33_text_16sp">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/text_size_16sp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textColorHint">@color/gray_c3</item>
        <item name="android:textColor">@color/black_33</item>
        <item name="android:maxLines">1</item>
    </style>

    <style name="black33_edit_16sp">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@null</item>
        <item name="android:textSize">@dimen/text_size_16sp</item>
        <item name="android:textColorHint">@color/gray_c3</item>
        <item name="android:textColor">@color/black_33</item>
    </style>

    <style name="theme_button">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/common40dp</item>
        <item name="android:layout_marginLeft">@dimen/common37dp</item>
        <item name="android:layout_marginRight">@dimen/common37dp</item>
        <item name="android:layout_marginTop">@dimen/common23dp</item>
        <item name="android:background">@drawable/selector_login_button</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/text_size_18sp</item>
    </style>
</resources>